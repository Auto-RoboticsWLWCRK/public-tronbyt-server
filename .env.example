#SERVER_HOSTNAME_OR_IP="M1Pro.local" # UNCOMMENT AND SET
SERVER_PORT=8000
SERVER_PROTOCOL="http"
SYSTEM_APPS_REPO="https://github.com/tronbyt/apps.git"
PRODUCTION=1
LOG_LEVEL=WARNING
#WEB_CONCURRENCY=4
#ADMIN_PASSWORD=pezzward
#MAX_USERS=100 # Default is 100

# User Registration Settings
# Set to "1" to enable open user registration (anyone can create an account)
# Set to "0" (default) to only allow admin users to create new accounts
#ENABLE_USER_REGISTRATION=1

# ============================================
# SUPABASE MULTI-TENANT CONFIGURATION
# ============================================
# Set AUTH_MODE to "supabase" to enable Supabase authentication
# When using "local" mode (default), SQLite and fastapi-login are used
#AUTH_MODE=supabase

# Supabase project URL (required when AUTH_MODE=supabase)
# Example: https://xxxxx.supabase.co
#SUPABASE_URL=

# Supabase anonymous/public key (required when AUTH_MODE=supabase)
# Found in Supabase Dashboard > Project Settings > API
#SUPABASE_ANON_KEY=

# Supabase service role key (required when AUTH_MODE=supabase)
# IMPORTANT: Keep this secret! Never expose in client-side code
#SUPABASE_SERVICE_ROLE_KEY=

# ============================================
# RATE LIMITING CONFIGURATION
# ============================================
# Number of requests allowed per minute per IP
#RATE_LIMIT_REQUESTS=60

# Burst capacity for rate limiting
#RATE_LIMIT_BURST=10
